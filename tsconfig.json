{
  "compilerOptions": {

    "target": "es2017",
    "module": "commonjs",

    "outDir": "./dist",
    "rootDir": "./src",
    "allowJs":true,
    "strict": true,

    "strictPropertyInitialization": false,
  "baseUrl": "./src",
  "esModuleInterop": true,


    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,


    "forceConsistentCasingInFileNames": true,
    "lib": [ "es2017" ],
  }
}
